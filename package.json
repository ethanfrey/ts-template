{
  "name": "sample-project",
  "version": "0.0.1",
  "description": "sample js template: change package.json",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "tdd": "jest --watch",
    "test:debug": "node --inspect-brk ./node_modules/.bin/jest --runInBand --watch",
    "start": "webpack-dev-server --config webpack/demo.js",
    "build": "webpack --config webpack/build.js --output-filename=zliq.js --output-path=lib/",
    "build:min": "webpack -p --config webpack/build_min.js --output-filename=zliq.min.js --output-path=lib/",
    "build:all": "npm run build && npm run build:min",
    "build:lib": "npm run build && npm run build:min",
    "prepublishOnly": "npm test && rimraf lib/** && npm run build:lib"
  },
  "author": "Ethan Frey <ethanfrey@users.noreply.github.com>",
  "license": "Apache-2.0",
  "dependencies": {
    "deep-equal": "^1.0.1",
    "stacktrace-js": "^2.0.0"
  },
  "devDependencies": {
    "awesome-typescript-loader": "^3.4.1",
    "babel-core": "^6.23.1",
    "babel-jest": "^20.0.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-h-jsx": "^1.0.0",
    "babel-preset-babili": "0.0.12",
    "babel-preset-es2015": "^6.22.0",
    "babel-register": "^6.24.0",
    "css-loader": "^0.26.1",
    "jest": "^22.0.1",
    "node-sass": "^4.5.0",
    "null-loader": "^0.1.1",
    "rimraf": "^2.6.1",
    "sass-loader": "^5.0.1",
    "style-loader": "^0.13.1",
    "ts-jest": "^22.0.4",
    "typescript": "^2.0.0",
    "webpack": "2.6.1",
    "webpack-dev-server": "2.5.0",
    "webpack-node-externals": "^1.5.4",
    "worker-loader": "^0.8.0"
  },
  "repository": {
    "url": "https://github.com/ethanfrey/js-template.git",
    "type": "git"
  },
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "babel-jest",
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "node",
      "json"
    ],
    "setupFiles": [],
    "moduleNameMapper": {
      "\\.(css|scss|jpg|png)$": "<rootDir>/test/helpers/empty-module.js"
    },
    "collectCoverage": true,
    "coverageDirectory": "./",
    "coverageReporters": [
      "lcov"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/",
      "/lib/"
    ],
    "globals": {
      "ts-jest": {
        "useBabelrc": true
      }
    },
    "testURL": "http://localhost"
  }
}
